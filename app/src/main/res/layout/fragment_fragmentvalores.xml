<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.android.proyecto.fragments.fragmentvalores">

    <!-- TODO: Update blank fragment layout -->

    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingBottom="50dp">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="top|center">

            <TextView
                android:id="@+id/nombreitca"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="bottom|center"
                android:padding="10dp"
                android:text="Nombre U"
                android:textColor="@color/Brown"
                android:textAlignment="center"
                android:textAllCaps="false"
                android:textSize="22sp"
                android:textStyle="bold" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_below="@+id/textView3"
                android:gravity="bottom|center"
                android:paddingLeft="10dp"
                android:paddingRight="10dp"
                android:text="@string/mision"
                android:textColor="@color/Amber"
                android:textAppearance="@style/Base.TextAppearance.AppCompat.Large" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="bottom|center">

            <TextView
                android:id="@+id/misionitca"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="6dp"
                android:text="@string/vision"
                android:textAlignment="center"
                android:textAllCaps="false"
                android:textColor="@color/Brown"
                android:textSize="15sp"
                android:textStyle="bold" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:gravity="bottom|center"
                android:text="@string/vision"
                android:textColor="@color/Amber"
                android:textAppearance="@style/Base.TextAppearance.AppCompat.Large" />
        </TableRow>
        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="bottom|center">

            <TextView
                android:id="@+id/visionitca"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/textView3"
                android:layout_toEndOf="@+id/textView3"
                android:paddingLeft="10dp"
                android:paddingRight="10dp"
                android:text="Texto Vision"
                android:padding="6dp"
                android:textColor="@color/Brown"
                android:textAlignment="center"
                android:textAllCaps="false"
                android:textSize="15sp"
                android:textStyle="bold" />

        </TableRow>
    </TableLayout>


</FrameLayout>
